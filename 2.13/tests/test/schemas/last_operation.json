{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "OSB Last Operation",
    "description": "OSB Last Operation",
    "type": "object",
    "required": ["state"],
    "properties": {
        "state": {
            "description": "Operation state",
            "type": "string",
            "enum":["in progress", "succeeded", "failed"]
        },
        "description":{
            "type":"string",
            "pattern": "\\S+"
        }
    },
    "additionalProperties": true
}
